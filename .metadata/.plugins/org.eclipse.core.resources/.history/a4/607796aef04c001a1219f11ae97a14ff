function createDataset(fields, constraints, sortFields) {
	
	var dataset = DatasetBuilder.newDataset();
	
	dataset.addColumn("estado");
	dataset.addColumn("sigla");
	
	dataset.addRow("São Paulo", "BR");
	dataset.addRow("Veracruz", "MX");
	dataset.addRow("Buenos Aires", "AR");
	dataset.addRow("Santiago", "CL");
	dataset.addRow("Montevidéu", "UY");

	
	return dataset;
	
